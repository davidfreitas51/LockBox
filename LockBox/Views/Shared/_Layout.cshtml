<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LockBox</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <!-- Bootstrap Icons -->
    <link href="/docs/5.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <!-- Favicons -->
    <link rel="apple-touch-icon" href="/docs/5.3/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
    <link rel="icon" href="/docs/5.3/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
    <link rel="icon" href="/docs/5.3/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
    <link rel="manifest" href="/docs/5.3/assets/img/favicons/manifest.json">
    <link rel="mask-icon" href="/docs/5.3/assets/img/favicons/safari-pinned-tab.svg" color="#712cf9">
    <link rel="icon" href="/docs/5.3/assets/img/favicons/favicon.ico">
    <meta name="theme-color" content="#712cf9">
    <!-- Meta Tags -->
</head>
<body>
    <header style="background-color: darkgreen">
        <nav class="navbar navbar-expand-lg bg-body-tertiary rounded" aria-label="Thirteenth navbar example">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarsExample11" aria-controls="navbarsExample11" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse d-lg-flex" id="navbarsExample11">
                    <a asp-controller="Home" asp-action="Index" class="navbar-brand me-lg-3" style="color: white;">
                        <img src="~/lib/images/LockBoxLogo.png" alt="Logo" style="height: 20px; width: 20px; margin-right: 5px;">
                        LockBox
                    </a>
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#" style="color: white;">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#pricing" style="color: white;">Pricing</a>
                        </li>
                    </ul>
                    <div class="d-lg-flex ms-lg-auto">
                        <div>
                            <a asp-controller="Home" asp-action="CreateAccount" class="btn btn-primary me-2" style="color: darkgreen; background-color: white; border-color: darkgreen;">Create account</a>
                            <a asp-controller="Home" asp-action="Login" class="btn btn-primary" style="background-color: darkgreen; border-color: darkgreen;">Login</a>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div >
        @RenderBody()
    </div>
    <footer class="pt-4 my-md-5 pt-md-5 border-top" style="text-align: center; overflow-x: hidden;">
        <div class="row">
            <div class="col-12 col-md mb-3">
                <img class="mb-2" src="/docs/5.3/assets/brand/bootstrap-logo.svg" alt="">
                <small class="d-block mb-3 text-body-secondary">LockBox© 2024</small>
                <small class="d-block mb-3 text-body-secondary">Made by: David Freitas</small>
                <a href="https://github.com/davidfreitas51">My GitHub</a>
                <a>    |    </a>
                <a href="https://www.linkedin.com/in/david-freitas51/">My LinkedIn</a>
            </div>
            <div class="col-6 col-md mb-3">
                <h5>Company</h5>
                <ul class="list-unstyled text-small">
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">About</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Events</li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Blog</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Partners</a></li>
                </ul>
            </div>
            <div class="col-6 col-md mb-3">
                <h5>Resources</h5>
                <ul class="list-unstyled text-small">

                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Resource center</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Community forums</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Security Compliance</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Newsfeed</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Survey room</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Subscribe to updates</a></li>
                </ul>
            </div>
            <div class="col-6 col-md mb-3">
                <h5>About</h5>
                <ul class="list-unstyled text-small">
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Team</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Locations</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Privacy</a></li>
                    <li class="mb-1"><a class="link-secondary text-decoration-none" href="#">Terms</a></li>
                </ul>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            setTimeout(function () {
                var password = '@TempData["MSG_Pass"]';
                if (password) {
                    const tempInput = document.createElement('input');
                    tempInput.value = password;
                    document.body.appendChild(tempInput);
                    tempInput.select();
                    document.execCommand('copy');
                    document.body.removeChild(tempInput);
                }
            }, 100);
        });
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
